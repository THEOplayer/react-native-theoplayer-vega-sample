{
  "name": "@amazon-devices/theovegademo",
  "version": "0.0.2",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf node_modules buildinfo.json dist build",
    "start": "react-native start",
    "app:debug": "npm-run-all build:debug install:debug app:launch",
    "app:release": "npm-run-all build:release install:release app:launch",
    "app:launch": "kepler device launch-app",
    "app:terminate": "kepler device terminate-app",
    "app:uninstall": " kepler device uninstall-app",
    "app:relaunch": "npm run app:terminate; npm run app:launch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "build:debug": "react-native build-kepler --build-type Debug",
    "install:debug": "kepler device install-app --directory . --buildType Debug",
    "build:release": "react-native build-kepler --build-type Release",
    "install:release": "kepler device install-app --directory . --buildType Release",
    "build": "npm-run-all build:release",
    "release": "npm-run-all lint test build:app"
  },
  "dependencies": {
    "@amazon-devices/kepler-media-controls": "^1.0.17",
    "@amazon-devices/react-native-kepler": "^2",
    "@amazon-devices/react-native-w3cmedia": "^2",
    "@amazon-devices/react-navigation__core": "^2",
    "@amazon-devices/react-navigation__native": "^2",
    "@amazon-devices/react-navigation__stack": "^2",
    "@theoplayer/react-native-drm": "^1.10.0",
    "@theoplayer/react-native-ui": "^0.21.2",
    "@theoplayer/react-native-vega": "file:lib/theoplayer-react-native-vega-1.0.0-beta.19.tgz",
    "@theoplayer/theomux-vega": "file:lib/theoplayer-theomux-vega-1.0.0.tgz",
    "@theoplayer/vega": "file:lib/theoplayer-vega-1.0.0-beta.19.tgz",
    "react": "18.2.0",
    "react-native": "0.72.0",
    "react-native-svg": "npm:@amazon-devices/react-native-svg@^2.0.1758683737"
  },
  "devDependencies": {
    "@amazon-devices/kepler-cli-platform": "^0",
    "@react-native-community/cli": "11.3.2",
    "@react-native/eslint-config": "0.72.2",
    "@react-native/metro-config": "^0.72.6",
    "@testing-library/react-native": "^7.2.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^28.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "babel-jest": "^28.0.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.12.0",
    "jest": "^28.0.0",
    "metro-react-native-babel-preset": "^0.76.5",
    "mustache": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^28.0.0",
    "typescript": "4.8.4"
  },
  "overrides": {
    "@theoplayer/react-native-ui": {
      "react-native-svg": "$react-native-svg"
    }
  },
  "kepler": {
    "projectType": "application",
    "appName": "THEOVegaDemo",
    "targets": [
      "tv"
    ]
  }
}
